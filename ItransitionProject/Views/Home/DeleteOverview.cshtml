@model ItransitionProject.Models.Overview
<form method="post">           
    <img src="@Url.Content(Model.ImageUrl)" class="card-img-top"/>
    <div class="body">
        <div class="title">
            <h5>@Model.Title</h5>
        </div>
        <div class="text">
            <p>@Model.TextOverview</p>
        </div>
        <div class="text">
            <p>@Html.DisplayFor(model=>model.FkgroupNavigation.GroupName)</p>
        </div>
        <div class="text">
            <p>@Html.DisplayFor(model=>model.FkuserNavigation.UserName)</p> 
        </div>
        <div class="text">
           @foreach(var item in Model.OverTags)
           {
            <p>@Html.DisplayFor(model=>item.FktagNavigation.TagName)</p>
           }
        </div>
    </div>          
        
                <button name="id" class="btn btn-primary" asp-action="Delete" asp-controller="Home" value="@Model.Idoverview">Удалить</button>
        
</form>
